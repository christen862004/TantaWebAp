@model Department
@{
    ViewData["Title"] = "New";
}

<h1>New</h1>
<!--Get : url :/Department/SaveNew?Name=sd&ManagerName=ajha-->
<form method="post" action="/Department/SaveNew">
    <div>
        <label>Name</label>
        <input type="text" name="Name" id="Name" value="@Model?.Name" class="form form-control" />
    </div>

    <div>
        <label>Manager Name</label>
        <input type="text" name="ManagerName" id="ManagerName" value="@Model?.ManagerName" class="form form-control" />
    </div>

    <input type="submit" value="create" class="btn btn-success" />
</form>
<a href="/Department/index">Back To List</a>